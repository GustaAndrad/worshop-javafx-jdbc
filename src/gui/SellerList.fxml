            <TableColumn fx:id="tableColumnEmail" prefWidth="92.0" text="Email" />
            <TableColumn fx:id="tableColumnBirthDate" prefWidth="75.0" text="BirthDate" />
            <TableColumn fx:id="tableColumnBaseSalary" prefWidth="84.79998779296875" text="Base Salary" />
            <TableColumn fx:id="tableColumnDepartment" text="Department" />
